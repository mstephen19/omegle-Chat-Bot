{
    "manifest_version": 2,
    "name": "omegleChatBot",
    "version_name": "2.0",
    "version": "0.002",
    "description": "Automatically send messages in Omegle!",
    "icons": {  "16": "icon16.png",
                "48": "icon48.png",
                "128": "icon128.png" },
    "content_scripts": [
        {
            "matches": ["https://www.omegle.com/*"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "omegleChatBot"
    }
}